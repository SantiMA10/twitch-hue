    "@hapi/hapi": "^20.0.1",
    "@streamdevs/lights-lul": "^3.5.3",
    "dotenv": "^8.2.0",
    "twitch": "^4.2.6",
    "twitch-pubsub-client": "^4.2.6"
  }
}
