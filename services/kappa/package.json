    "jest": "^26.0.1",
    "nock": "^13.0.4",
    "nodemon": "^2.0.3",
    "ts-jest": "^26.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "@google-cloud/firestore": "^4.4.0",
    "@google-cloud/tasks": "^2.1.1",
    "@hapi/hapi": "^20.0.1",
    "dotenv": "^8.2.0",
